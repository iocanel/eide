import javax.persistence.Entity;
import javax.persistence.Column;

import io.quarkus.hibernate.orm.panache.PanacheEntity;

@Entity
public class `(file-name-nondirectory (file-name-sans-extension (buffer-file-name)))` extends PanacheEntity {

  @Column
  String name;

  public `(file-name-nondirectory (file-name-sans-extension (buffer-file-name)))`() {
  }

  public `(file-name-nondirectory (file-name-sans-extension (buffer-file-name)))`(String name) {
    this.name=name;
  }
}